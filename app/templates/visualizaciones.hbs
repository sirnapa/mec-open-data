<div id="visualizaciones" class="row">

  {{#each vista in model}}
      <div class="vista no-cargada col-sm-6 col-md-4 enlace" onclick="window.open('{{unbound vista.enlace}}','_self');">
        <div class="thumbnail well">
          <img src="{{unbound vista.imagen}}" alt="{{unbound vista.titulo}}" class="img-responsive">
          <div class="caption">
            <h3>{{vista.titulo}}</h3>
            <p>{{vista.descripcion}}</p>
            <p><a href="{{unbound vista.enlace}}" class="btn btn-primary" role="button">Ir a la visualizaci√≥n</a></p>
          </div>
        </div>
      </div>
  {{/each}}

</div>
